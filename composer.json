{
    "name": "graham-campbell/manager",
    "description": "Manager Provides Some Manager Functionality For Laravel 4.1/4.2",
    "keywords": ["laravel", "framework", "manager", "connector", "interface", "Manager", "Laravel Manager", "Laravel-Manager", "Graham Campbell", "GrahamCampbell"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "illuminate/config": "4.*|5.*",
        "illuminate/support": "4.*|5.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\Manager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\Manager\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
